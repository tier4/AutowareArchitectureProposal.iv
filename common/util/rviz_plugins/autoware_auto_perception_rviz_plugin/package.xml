<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>autoware_rviz_plugins</name>
    <version>0.0.2</version>
    <description>Contains plugins to visualize object detection outputs</description>
    <maintainer email="opensource@apex.ai">Apex.AI, Inc.</maintainer>
    <license>Apache 2.0</license>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>autoware_auto_cmake</buildtool_depend>

    <build_depend>qtbase5-dev</build_depend>

    <exec_depend>libqt5-widgets</exec_depend>

    <depend>autoware_auto_msgs</depend>
    <depend>rviz_common</depend>
    <depend>rviz_default_plugins</depend>

    <build_depend>autoware_auto_common</build_depend>

    <test_depend>ament_lint_auto</test_depend>
    <test_depend>ament_lint_common</test_depend>

    <export><build_type>ament_cmake</build_type></export>
</package>
